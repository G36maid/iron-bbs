  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            p.id, p.title, p.content, p.author_id, p.created_at, p.updated_at, p.published,\n            u.username as author_username, u.email as author_email\n        FROM posts p\n        JOIN users u ON p.author_id = u.id\n        WHERE p.id = $1 AND p.published = true\n        ",
